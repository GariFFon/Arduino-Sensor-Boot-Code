#define a 1
#define b 2
#define c 3
#define d 4
#define e 5
#define f 6
#define g 7
#define h 8
void setup() {
  // put your setup code here, to run once:
pinMode(a,OUTPUT);
pinMode(b,OUTPUT);
pinMode(c,OUTPUT);
pinMode(d,OUTPUT);
pinMode(d,OUTPUT);
pinMode(f,OUTPUT);
pinMode(g,OUTPUT);
pinMode(h,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
digitalWrite(a,HIGH);
delay(9);
digitalWrite(a,LOW);
delay(2);
digitalWrite(b,HIGH);
delay(10);
digitalWrite(b,LOW);
delay(1);
digitalWrite(c,HIGH);
delay(10);
digitalWrite(c,LOW);
delay(1);
digitalWrite(d,HIGH);
delay(10);
digitalWrite(d,LOW);
delay(1);
digitalWrite(e,HIGH);
delay(10);
digitalWrite(e,LOW);
delay(1);
digitalWrite(f,HIGH);
delay(10);
digitalWrite(f,LOW);
delay(1);
digitalWrite(g,HIGH);
delay(10);
digitalWrite(g,LOW);
delay(1);
digitalWrite(h,HIGH);
delay(10);
digitalWrite(h,LOW);
delay(1);
}
